{
    "_version": "1.32.0",
    "sap.app": {
        "id": "sapui5.rk.onlyui",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "sapui5.rk.onlyui.view.View1",
            "type": "XML",
            "async": true,
            "id": "View1"
        },
        "dependencies": {
            "minUI5Version": "1.94.1",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sapui5.rk.onlyui.i18n.i18n"
                }
            },
         "cdata": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/cdata.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": false,
                "viewPath": "sapui5.rk.onlyui.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "View1",
                    "pattern": "v1",
                    "titleTarget": "TargetView1",
                    "greedy": false,
                    "target": [
                        "TargetView1"
                    ]
                },
                {
                    "name": "View2",
                    "pattern": "v2",
                    "titleTarget": "TargetView2",
                    "greedy": false,
                    "target": [
                        "TargetView2"
                    ]
                },
                {
                    "name": "View3",
                    "pattern": "v3",
                    "titleTarget": "TargetView3",
                    "greedy": false,
                    "target": [
                        "TargetView3"
                    ]
                }
            ],
            "targets": {
                "TargetView1": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": true,
                     "viewName": "View1",
                     "viewLevel":1
                },
                "TargetView2": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": true,
                    "viewName": "View2",
                     "viewLevel": 2
                },
                "TargetView3": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": true,
                    "viewName": "View3",
                     "viewLevel": 3
                }
                
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sapui5-rk-onlyui"
    }
}